{
  "name": "automart",
  "version": "1.0.0",
  "description": "'Automobile online marketplace'",
  "main": "build/index.js",
  "scripts": {
    "dev": "NODE_ENV=development nodemon --exec babel-node server/index.js",
    "test": "nyc mocha --require babel-core/register server/test/**/**.spec.js --exit || true",
    "build": "babel server --out-dir server/build",
    "prebuild": "rm -rf server/build",
    "prestart": "npm run lint && npm run build",
    "start": "NODE_ENV=production node server/build/index.js",
    "lint": "eslint server",
    "coverage": "nyc report --reporter=text-lcov | coveralls"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tvpeter/automart.git"
  },
  "author": "'Tyonum Peter'",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/tvpeter/automart/issues"
  },
  "homepage": "https://github.com/tvpeter/automart#readme",
  "dependencies": {
    "bcrypt": "3.0.6",
    "cloudinary": "1.14.0",
    "debug": "4.1.1",
    "dotenv": "8.0.0",
    "express": "4.17.0",
    "jsonwebtoken": "8.5.1",
    "multer": "1.4.1",
    "winston": "3.2.1"
  },
  "devDependencies": {
    "@babel/core": "7.4.4",
    "@babel/node": "7.2.2",
    "babel-cli": "6.26.0",
    "babel-preset-env": "1.7.0",
    "babel-register": "6.26.0",
    "babel-watch": "7.0.0",
    "chai": "4.2.0",
    "chai-http": "4.3.0",
    "coveralls": "3.0.3",
    "eslint": "5.16.0",
    "eslint-config-airbnb-base": "13.1.0",
    "eslint-plugin-import": "2.17.2",
    "istanbul": "0.4.5",
    "mocha": "6.1.4",
    "mocha-lcov-reporter": "1.3.0",
    "nodemon": "1.19.1",
    "nyc": "14.1.1",
    "supertest": "4.0.2"
  },
  "nyc": {
    "exclude": [
      "**/test/**"
    ]
  }
}